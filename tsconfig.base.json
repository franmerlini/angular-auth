        "libs/web/shared/utils/app-init/src/index.ts"
      ],
      "@angular-auth/web/home/feature": ["libs/web/home/feature/src/index.ts"],
      "@angular-auth/web/shell/feature": [
        "libs/web/shell/feature/src/index.ts"
      ],
      "@angular-auth/web/shell/ui/footer": [
        "libs/web/shell/ui/footer/src/index.ts"
      ],
      "@angular-auth/web/shell/ui/header": [
        "libs/web/shell/ui/header/src/index.ts"
      ],
      "@angular-auth/web/shell/ui/layout": [
        "libs/web/shell/ui/layout/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
