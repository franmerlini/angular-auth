{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular-auth/libs/common": ["libs/common/src/index.ts"],
      "@angular-auth/libs/web/auth/data-access/store": [
        "libs/web/auth/data-access/store/src/index.ts"
      ],
      "@angular-auth/libs/web/auth/feature/login": [
        "libs/web/auth/feature/login/src/index.ts"
      ],
      "@angular-auth/libs/web/auth/feature/register": [
        "libs/web/auth/feature/register/src/index.ts"
      ],
      "@angular-auth/libs/web/auth/ui/login-form": [
        "libs/web/auth/ui/login-form/src/index.ts"
      ],
      "@angular-auth/libs/web/auth/ui/register-form": [
        "libs/web/auth/ui/register-form/src/index.ts"
      ],
      "@angular-auth/libs/web/auth/utils": ["libs/web/auth/utils/src/index.ts"],
      "@angular-auth/libs/web/shared/data-access/api": [
        "libs/web/shared/data-access/api/src/index.ts"
      ],
      "@angular-auth/libs/web/shared/store": [
        "libs/web/shared/store/src/index.ts"
      ],
      "@angular-auth/libs/web/shared/utils/app-config": [
